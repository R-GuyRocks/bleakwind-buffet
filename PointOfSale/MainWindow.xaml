        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PointOfSale"
        mc:Ignorable="d"
        Title="MainWindow" Height="539" Width="765" MaxHeight="539" MaxWidth="765">
    <DockPanel Name="mainDockPanel">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="500"/>
                <ColumnDefinition Width="250"/>
            </Grid.ColumnDefinitions>
            <Border Name="menuBorder" Grid.Column="0">
                <!--             <local:MenuSelection x:Name="menuSelection" Grid.Column="0" VerticalAlignment="Top" Loaded="MenuSelection_Loaded"/> -->
            </Border>
            <Border x:Name="orderBorder" Grid.Column="1"/>
        </Grid>
    </DockPanel>
</Window>
